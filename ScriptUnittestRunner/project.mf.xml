<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<manifest>
    <application name="ScriptUnittestRunner">
        <crown name="ScriptUnittestRunner">
            <desc>user script</desc>
            <serves>
                <function name="runTests">
                    <desc>Starts test run</desc>
                </function>
            </serves>
        </crown>
        <meta key="author">AppSpace Sample</meta>
        <meta key="version">2.5.0</meta>
        <meta key="licensed">false</meta>
        <meta key="priority">low</meta>
        <meta key="copy-protected">false</meta>
        <meta key="read-protected">false</meta>
        <meta key="tags">Testing</meta>
        <entry path="pages" default="ScriptUnittestRunner.msdd">
            <file type="application/zip">ScriptUnittestRunner.msdd</file>
        </entry>
        <entry path="scripts" default="ScriptUnittestRunner.lua">
            <file type="text/x-lua">ScriptUnittestRunner.lua</file>
        </entry>
    </application>
</manifest>
